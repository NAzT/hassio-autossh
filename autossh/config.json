{
  "name": "Autossh",
  "version": "1.0.1",
  "slug": "autossh",
  "description": "Create SSH tunnel to public server",
  "url": "https://github.com/nazt/hassio-autossh/tree/master/autossh",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "map": ["config:rw", "ssl"],
  "options": {
    "tunnel_host": "",
    "tunnel_remote_string": "$TUNNEL_PORT:localhost:8123"
  },
  "schema": {
    "tunnel_host": "str",
    "tunnel_remote_string": "str"
  }
}
